@model RegistcomViewModel

@{
    ViewBag.Title = "Task Recommendation";
}
<h1>Assign Task</h1>

<div class="row">
    <div class="col-md-12">
        <form method="post">
            <div asp-validation-summary="All" class="text-danger"> </div>
                <div class="form-group row">
                    <label asp-for="StaffEmail" class="col-sm-2 col-form-label"></label>
                    <select asp-items="@Model.StaffEmailList" asp-for="StaffEmail"   class="form-control">
                        <option  value="0">Select Email</option>
                        @*<option value="Male">Male</option>
                        <option value="Female">Female</option>*@
                    </select>
                </div>
                <div class="form-group">
                    <label asp-for="RecomTask"></label>
                    <input asp-for="RecomTask" class="form-control" />
                    <span asp-validation-for="RecomTask" class="text-danger"></span>
                </div>
                <br />
                <div> <button type="submit" class="btn btn-primary">Recommend</button></div>
           
        </form>
            </div>    
    </div>
